package aspirin.ui
{
	import flash.display.Sprite;
	
	public class UIComponentBase extends Sprite implements IUIComponent
	{
		protected var _depth : int = 0;
		
		protected var _state : String;
		
		public function PlayerUIComponentBase() : void
		{
			
		}
		
		public function setState( state : String ) : void
		{
			_state = state;
		}
		
		public function resize( w : int, h : int ) : void
		{
			
		}
		
		public function get depth() : int
		{
			return _depth;
		}
		
		public function set depth( n : int ) : void
		{
			_depth = n;
		}

	}
}